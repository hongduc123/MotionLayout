<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd"
        motion:staggered="0.65"
        motion:motionInterpolator="cubic(0.2,0.19,0.8,0.8)">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/view2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="312dp"
            android:layout_height="71dp"
            motion:layout_constraintTop_toBottomOf="@+id/view4"
            motion:layout_constraintHorizontal_bias="0.505"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="48dp"
            android:scaleX=".5"
            android:scaleY=".5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/view4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="312dp"
            android:layout_height="71dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:layout_constraintHorizontal_bias="0.494"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="76dp"
            android:scaleX=".5"
            android:scaleY=".5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="312dp"
            android:layout_height="71dp"
            motion:layout_constraintTop_toBottomOf="@+id/view2"
            motion:layout_constraintHorizontal_bias="0.505"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="48dp"
            android:scaleX=".5"
            android:scaleY=".5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:translationY="10dp"
            android:alpha="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint></Constraint>

    </ConstraintSet>
</MotionScene>